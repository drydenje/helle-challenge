     return x(temp);
    })
  .y(d => y(d.savePctg));

svg.append("path")
  .datum(helle.gameLog.reverse())
  .attr('fill', 'none')
  .attr('stroke', 'steelblue')
  .attr('stoke-width', 1)
  .attr('d', line);

// Append the SVG element.
container.append(svg.node());

